{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.3.5"
    }
  },
"routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "/404.html" }
  ],
"rewrites": [
    { "source": "/proxy/:match/:url*", "destination": "https://:match/:url*" },
    { "source": "/proxy/:match/:url*/", "destination": "https://:match/:url*/" }
  ]
}